@misc{kidgerEquinoxNeuralNetworks2021,
  title = {Equinox: Neural Networks in {{JAX}} via Callable {{PyTrees}} and Filtered Transformations},
  shorttitle = {Equinox},
  author = {Kidger, Patrick and Garcia, Cristian},
