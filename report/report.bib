  file = {C\:\\Users\\jeppe\\Zotero\\storage\\I74ZKZ8B\\Lundow and Campbell - 2018 - Hyperscaling breakdown and Ising spin glasses The.pdf;C\:\\Users\\jeppe\\Zotero\\storage\\ZSIYJGHM\\S037843711731172X.html}
